<% %>
<thead>
	<tr><th>
		Time
	</th><th>
		Finding
	</th><th>
		Remark
	</th><th>
		Status
	</th><th>
		Severity
	</th></tr>
</thead>
<tbody>
	<% for ( var i = 0; i < this.length ; i++ ) { %>
		<tr <%= this[i] %>>
			<%== $.View(
				'//seccubus/history/table/views/history.ejs',
				this[i],
				{
					asHTML	: asHTML,
					previous: this[i+1]
				}
			) %>
		</tr>
	<% } %>
</tbody>
